packetdiag {
  colwidth = 32

  8-15: Reply SpW Address [color = LightSkyBlue ]
  16-23: ... [color = LightSkyBlue ]
  24-31: Reply SpW Address [color = LightSkyBlue ]
  32-39: Initiator Logical Address [color = LightCoral]
  40-47: Protocol Identifier [color = LemonChiffon ]
  48-55: Instruction
  56-63: Status
  64-71: Target Logical Address
  72-79: Transaction Identifier(MS)
  80-87: Transaction Identifier(LS)
  88-95: Header CRC
  96-99: EOP [color = LightCoral]
}
